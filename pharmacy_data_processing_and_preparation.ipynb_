': [f\"{np.random.randint(8, 22)}:{np.random.randint(0, 59):02d}\" for _ in range(n_rows)],\n","    'prod_name': np.random.choice(products, n_rows),\n","    'qty': np.random.randint(1, 10, n_rows).astype(object), # сделаем object чтобы испортить тип\n","    'price_rub': np.random.uniform(50.0, 5000.0, n_rows),\n","    'client_loyalty_num': [f\"ID-{np.random.randint(10000, 99999)}\" if np.random.random() > 0.3 else np.nan for _ in range(n_rows)],\n","    'branch_loc': np.random.choice(branches, n_rows),\n","    'is_online': np.random.choice([0, 1], n_rows),\n","    'pay_method': np.random.choice(payment_methods, n_rows, p=[0.2, 0.5, 0.2, 0.1]),\n","    'trans_status': np.random.choice(statuses, n_rows, p=[0.85, 0.1, 0.03, 0.02]),\n","    'manager_comment': ['OK' if np.random.random() > 0.1 else np.nan for _ in range(n_rows)],\n","    'tech_log_dump': ['log_data_x86' for _ in range(n_rows)], # трэш столбец\n","    'temp_col_1': np.random.randn(n_rows), # трэш столбец\n","    'temp_col_2': np.random.randn(n_rows), # трэш столбец\n","    'discount_applied': np.random.choice(['Yes', 'No', 0, 1], n_rows), # смешан. типы\n","    'region_code': [77] * n_rows,\n","    'pharmacy_rating': np.random.randint(1, 6, n_rows),\n","    'supplier_id': np.random.randint(100, 105, n_rows),\n","    'prescription_req': np.random.choice([True, False], n_rows)}\n","\n","\n","df = pd.DataFrame(data) # доб. грязь вручную для демонстрации навыков\n","\n","df['d_date'] = df['d_date'].astype(str)  # портим часть дат, превращая их в строки\n","\n","df.loc[df.sample(50).index, 'price_rub'] = np.nan # доб. пропуски в цену\n","\n","df.shape"]},{"cell_type":"markdown","source":["## Блок 2: Первичный осмотр данных"],"metadata":{"id":"wOlvxWQ5Es2h"}},{"cell_type":"code","source":["display(df.head(3))\n","\n","df.info()\n","\n","# Проверка: сколько памяти занимает датафрейм до оптимизации\n","initial_memory = df.memory_usage(deep=True).sum() / 1024**2\n","print(f\"\\nИсходный объем памяти: {initial_memo